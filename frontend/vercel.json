{
    "version": 2,
    "builds": [
      {
        "src": "vite.config.ts",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/media/(.*)",
        "dest": "https://your-backend-url.com/media/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/dist/$1"
      }
    ]
  }